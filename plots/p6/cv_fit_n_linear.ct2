#!/usr/bin/ctioga2 -f

xlabel 'dimensions'
ylabel '$n$'

marker bullet
marker-scale 0.40
line-style no

load cv_avrami_instant.csv
plot @'$2:$4:yerr=$5*2'  /legend "Instant"

load cv_avrami_sporadic.csv
plot @'$2:$4:yerr=$5*2'  /legend "Sporadic" 

math /xrange 1:7 /samples 100


marker none
line-style solid
plot 'x'	/color Red 		/legend '$n=d$'

marker none
line-style solid
plot 'x+1'	/color Green	/legend '$n=d+1$'
