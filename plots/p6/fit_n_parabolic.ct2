#!/usr/bin/ctioga2 -f

xlabel 'dimensions'
ylabel '$n$'

marker bullet
marker-scale 0.40
line-style no

load avrami_instant_parabolic.csv
plot @'$2:$4:yerr=$5*2' /legend "Instant parabolic"

load avrami_sporadic_parabolic.csv
plot @'$2:$4:yerr=$5*2' /legend "Sporadic parabolic"


math /xrange 1:7 /samples 100


marker none
line-style solid
plot '1.75*x'	/color Red 		/legend '$n=1.75 d$'

marker none
line-style solid
plot '0.65*x+1'	/color Green	/legend '$n=0.65d+1$'
